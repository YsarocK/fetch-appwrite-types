{
  "name": "fetch-appwrite-types",
  "version": "1.2.3",
  "description": "Appwrite types file maker",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "type": "module",
  "scripts": {
    "deploy": "npm run lint && np",
    "dev": "npx ts-node src/main.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "prepare": "npm run lint && npm run build && npm run test",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "check-updates": "npx npm-check-updates",
    "check-updates:minor": "npx npm-check-updates --target minor",
    "check-updates:patch": "npx npm-check-updates --target patch"
  },
  "bin": "dist/bin/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YsarocK/fetch-appwrite-types"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "typescript",
    "npm",
    "template",
    "ts"
  ],
  "author": "Etienne Moureton",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "ansi-regex": ">=6.0.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "dts-dom": "^3.7.0",
    "node-appwrite": "^8.2.0",
    "ts-node": "^10.9.1"
  }
}
